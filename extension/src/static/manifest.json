{
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "default_locale": "zh_CN",
  "background": {
    "service_worker": "backgroundloader.js"
  },
  "options_ui": {
    "page": "option.html",
    "open_in_tab": true
  },
  "action":{
    "default_title": "My Bookmarks",
    "default_icon": "128.png"
  },
  "icons": {
    "16": "128.png",
    "48": "128.png",
    "128": "128.png"
  },
  "manifest_version": 3,
  "permissions": ["tabs", "bookmarks","storage","alarms"],
  "version": "0.0.1",
  "host_permissions": ["*://*/*"]
}
